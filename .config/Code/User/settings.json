{
    "vim.easymotion": true,
    "vim.useCtrlKeys": true,
    "gitlens.keymap": "chorded",
    "window.newWindowDimensions": "maximized",
    "latex-workshop.view.pdf.viewer": "tab",
    "git.promptToSaveFilesBeforeCommit": "always",
    "editor.fontFamily": "'Hack', 'Source Han Sans CN', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.suggest.localityBonus": true,
    "go.formatTool": "gofmt",
    "files.enableTrash": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "files.trimFinalNewlines": true,
    "editor.acceptSuggestionOnEnter": "off",
    "files.insertFinalNewline": true,
    "editor.smoothScrolling": true,
    "go.lintOnSave": "off",
    "search.useIgnoreFiles": false,
    "editor.wordWrapColumn": 120,
    "files.trimTrailingWhitespace": true,
    "rust.all_features": true,
    "editor.mouseWheelZoom": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/coverage": true,
        "**/package-lock.json": true
    },
    "files.associations": {
        "*.json": "jsonc"
    },
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "fish": {
            "path": "fish"
        },
    },
    "terminal.integrated.defaultProfile.linux": "fish",
    "workbench.iconTheme": "vscode-icons",
    "extensions.ignoreRecommendations": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.configuration.checkProjectSettingsExclusions": false,
    "editor.minimap.maxColumn": 100,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "rust-analyzer.files.exclude": [
        ".history"
    ],
    "rust-analyzer.notifications.cargoTomlNotFound": false,
    "rust-analyzer.updates.channel": "stable",
    "rust.unstable_features": true,
    "typescript.tsserver.log": "off",
    "remote.SSH.useLocalServer": false,
    "python.pythonPath": "/bin/python2",
    "rust-client.engine": "rust-analyzer",
    "python.languageServer": "Pylance",
    "window.restoreWindows": "none",
    "rust-client.autoStartRls": false,
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.cppStandard": "c++20",
    "go.useLanguageServer": true,
    "editor.formatOnSaveMode": "modifications",
    "[go]": {
        "editor.formatOnSave": true
    },
    "cmake.configureOnOpen": true,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.enableTelemetry": true,
    "julia.format.indent": 2,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "s"
            ],
            "after": [
                "leader",
                "leader",
                "s"
            ]
        },
        {
            "before": [
                "leader",
                "j"
            ],
            "after": [
                "leader",
                "leader",
                "j"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "after": [
                "leader",
                "leader",
                "k"
            ]
        },
        {
            "before": [
                "leader",
                "b"
            ],
            "after": [
                "leader",
                "leader",
                "b"
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "after": [
                "leader",
                "leader",
                "w"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "s"
            ],
            "after": [
                "leader",
                "leader",
                "s"
            ]
        },
        {
            "before": [
                "leader",
                "j"
            ],
            "after": [
                "leader",
                "leader",
                "j"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "after": [
                "leader",
                "leader",
                "k"
            ]
        },
        {
            "before": [
                "leader",
                "b"
            ],
            "after": [
                "leader",
                "leader",
                "b"
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "after": [
                "leader",
                "leader",
                "w"
            ]
        }
    ],
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.wordWrap": "off",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "yaml": true
    },
    "go.toolsManagement.autoUpdate": true,
    "python.defaultInterpreterPath": "/bin/python3",
    "julia.symbolCacheDownload": true,
    "html.autoCreateQuotes": false,
    "html.autoClosingTags": false,
    "jupyter.askForKernelRestart": false,
    "editor.renderWhitespace": "none",
    "remote.SSH.configFile": "/home/mao/.ssh/config",
    "remote.SSH.remotePlatform": {
        "dpdk.p.maowtm.org": "linux",
        "dpdk-devbox.az.maowtm.org": "linux",
        "dpdk-devbox-2.az.maowtm.org": "linux",
        "linux-devbox-vm": "linux"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "css.format.spaceAroundSelectorSeparator": true,
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "svelte.enable-ts-plugin": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.renderControlCharacters": false,
    "excalidraw.theme": "auto",
    "accessibility.signals.terminalBell": {
        "sound": "on"
    },
    "terminal.integrated.enableVisualBell": true,
    "diffEditor.renderSideBySide": false,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "redhat.telemetry.enabled": true,
    "cmake.debugConfig": {},
    "workbench.colorTheme": "GitHub Light",
    "editor.mouseWheelScrollSensitivity": 2,
    "terminal.integrated.localEchoEnabled": "off",
    "vim.textwidth": 74,
    "gitlens.rebaseEditor.ordering": "asc",
    "workbench.secondarySideBar.showLabels": false,
    "files.autoSave": "onFocusChange",
    "editor.inlayHints.enabled": "off",
    "clangd.arguments": [
        "-header-insertion=never"
    ],
    "github.copilot.nextEditSuggestions.enabled": true,
    "diffEditor.hideUnchangedRegions.contextLineCount": 6,
    "[diff]": {
        "files.trimTrailingWhitespace": false,
        "editor.renderWhitespace": "all"
    },
    "[eml]": {
        "files.trimTrailingWhitespace": false,
        "editor.renderWhitespace": "all"
    }
}
