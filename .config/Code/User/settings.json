{
    "vim.easymotion": true,
    "vim.useCtrlKeys": true,
    "gitlens.keymap": "chorded",
    "workbench.colorTheme": "GitHub Light",
    "editor.tabSize": 2,
    "window.newWindowDimensions": "maximized",
    "latex-workshop.view.pdf.viewer": "tab",
    "git.promptToSaveFilesBeforeCommit": "always",
    "editor.fontFamily": " 'Hack', 'Source Han Sans', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.suggest.localityBonus": true,
    "go.formatTool": "gofmt",
    "files.enableTrash": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "files.trimFinalNewlines": true,
    "editor.acceptSuggestionOnEnter": "off",
    "files.insertFinalNewline": true,
    "files.autoSave": "off",
    "editor.smoothScrolling": true,
    "terminal.integrated.enableBell": true,
    "go.lintOnSave": "off",
    "search.useIgnoreFiles": false,
    "editor.wordWrapColumn": 120,
    "files.trimTrailingWhitespace": true,
    "rust.all_features": true,
    "editor.mouseWheelZoom": true,
    "window.titleBarStyle": "custom",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/coverage": true,
        "**/package-lock.json": true
    },
    "files.associations": {
        "*.json": "jsonc"
    },
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "fish": {
            "path": "fish"
        },
    },
    "terminal.integrated.defaultProfile.linux": "fish",
    "workbench.iconTheme": "vscode-icons",
    "extensions.ignoreRecommendations": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.configuration.checkProjectSettingsExclusions": false,
    "editor.minimap.maxColumn": 100,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "rust-analyzer.files.exclude": [
        ".history"
    ],
    "rust-analyzer.notifications.cargoTomlNotFound": false,
    "rust-analyzer.updates.channel": "stable",
    "rust.unstable_features": true,
    "workbench.activityBar.visible": true,
    "typescript.tsserver.log": "off",
    "remote.SSH.useLocalServer": false,
    "python.pythonPath": "/bin/python2",
    "rust-client.engine": "rust-analyzer",
    "python.languageServer": "Pylance",
    "window.restoreWindows": "none",
    "C_Cpp.clang_format_style": "LLVM",
    "rust-client.autoStartRls": false,
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.cppStandard": "c++20",
    "go.useLanguageServer": true,
    "editor.formatOnSaveMode": "modifications",
    "[go]": {
        "editor.formatOnSave": true
    },
    "cmake.configureOnOpen": true,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.enableTelemetry": true,
    "julia.format.indent": 2,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["leader", "s"],
            "after": ["leader", "leader", "s"]
        },
        {
            "before": ["leader", "j"],
            "after": ["leader", "leader", "j"]
        },
        {
            "before": ["leader", "k"],
            "after": ["leader", "leader", "k"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["leader", "s"],
            "after": ["leader", "leader", "s"]
        },
        {
            "before": ["leader", "j"],
            "after": ["leader", "leader", "j"]
        },
        {
            "before": ["leader", "k"],
            "after": ["leader", "leader", "k"]
        }
    ],
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.wordWrap": "off",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true
    },
    "go.toolsManagement.autoUpdate": true,
    "python.defaultInterpreterPath": "/bin/python3",
    "julia.symbolCacheDownload": true
}
