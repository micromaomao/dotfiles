{
    "vim.easymotion": true,
    "vim.useCtrlKeys": true,
    "gitlens.keymap": "chorded",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.colorTheme": "GitHub Light",
    "editor.tabSize": 2,
    "window.newWindowDimensions": "maximized",
    "latex-workshop.view.pdf.viewer": "tab",
    "git.promptToSaveFilesBeforeCommit": "always",
    "editor.fontFamily": " 'Hack', 'Source Han Sans', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "workbench.statusBar.feedback.visible": false,
    "editor.suggest.localityBonus": true,
    "go.formatTool": "gofmt",
    "files.enableTrash": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "files.trimFinalNewlines": true,
    "editor.acceptSuggestionOnEnter": "off",
    "files.insertFinalNewline": true,
    "files.useExperimentalFileWatcher": true,
    "files.autoSave": "off",
    "go.gopath": "/home/mao/go",
    "editor.smoothScrolling": true,
    "terminal.integrated.enableBell": true,
    "terminal.integrated.rendererType": "canvas",
    "go.lintOnSave": "off",
    "search.useIgnoreFiles": false,
    "editor.wordWrapColumn": 120,
    "files.trimTrailingWhitespace": true,
    "rust.all_features": true,
    "editor.mouseWheelZoom": true,
    "window.titleBarStyle": "custom",
    "peacock.affectActivityBar": true,
    "peacock.affectStatusBar": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/coverage": true,
        "**/package-lock.json": true
    },
    "files.associations": {
        "*.json": "jsonc"
    },
    "terminal.integrated.shell.linux": "/usr/bin/fish",
    "workbench.iconTheme": "vscode-icons",
    "extensions.ignoreRecommendations": false,
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "git.enableCommitSigning": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "editor.minimap.maxColumn": 100,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "rust-analyzer.files.exclude": [
        ".history"
    ],
    "rust-analyzer.notifications.cargoTomlNotFound": false,
    "rust-analyzer.updates.channel": "nightly",
    "rust.unstable_features": true,
    "workbench.activityBar.visible": true,
    "typescript.tsserver.log": "verbose",
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
        "mc.maowtm.org": "linux",
        "csrw.cs.ucl.ac.uk": "linux"
    },
    "python.pythonPath": "/bin/python2",
    "rust-client.engine": "rust-analyzer",
    "python.languageServer": "Pylance",
    "window.restoreWindows": "none",
    "C_Cpp.clang_format_style": "LLVM",
    "rust-client.autoStartRls": false,
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.cppStandard": "c++20",
    "go.useLanguageServer": true,
    "editor.formatOnSaveMode": "modifications",
    "[go]": {
        "editor.formatOnSave": true
    },
    "cmake.configureOnOpen": true,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.enableTelemetry": true,
    "julia.format.indent": 2,
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "diffEditor.renderSideBySide": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["leader", "s"],
            "after": ["leader", "leader", "s"]
        },
        {
            "before": ["leader", "j"],
            "after": ["leader", "leader", "j"]
        },
        {
            "before": ["leader", "k"],
            "after": ["leader", "leader", "k"]
        }
    ]
}
